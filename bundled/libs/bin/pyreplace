#!/Users/olle/Projects/pyrefact-vscode-extension/.nox/build_package/bin/python
# -*- coding: utf-8 -*-
import re
import sys
from pyrefact.pattern_matching import pyreplace_main
if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw|\.exe)?$', '', sys.argv[0])
    sys.exit(pyreplace_main())
